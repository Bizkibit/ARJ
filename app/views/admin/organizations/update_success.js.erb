$('#<%= j dom_id(@organization)%> .btn').remove()

if ("<%= @organization.rejected? %>" === "true") {
  $("#<%= j dom_id(@organization)%>").prependTo($("#rejected_organizations")).hide().fadeIn()
} else {
  $("#<%= j dom_id(@organization)%>").prependTo($("#approved_organizations")).hide().fadeIn()
}

